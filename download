  });
  $("#freebsd_arm").on("click", function () {
    $(this).parent().toggle();
    $("#other-selected").text("FreeBSD (ARM)");
    $("#other-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-freebsd-arm.tgz`);
  });
  $("#freebsd_amd64").on("click", function () {
    $(this).parent().toggle();
    $("#other-selected").text("FreeBSD (64-bit)");
    $("#other-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-freebsd-amd64.tgz`);
  });
  $("#freebsd_386").on("click", function () {
    $(this).parent().toggle();
    $("#other-selected").text("FreeBSD (32-bit)");
    $("#other-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-freebsd-386.tgz`);
  });
  $("#solaris_amd64").on("click", function () {
    $(this).parent().toggle();
    $("#other-selected").text("Solaris (AMD64)");
    $("#other-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-solaris-amd64.tgz`);
  });

  $(".tab").on("click", function () {
    const id = $(this).attr("id");
    $(".tab").addClass("hover:bg-gray-200");
    $(this).removeClass("hover:bg-gray-200");
    $(".tab").children().removeClass("text-blue-800").addClass("text-gray-400");
    $(".tab")
      .children()
      .parent()
      .removeClass("bg-white")
      .addClass("bg-gray-100");
    $(this).children().removeClass("text-gray-400").addClass("text-blue-800");
    $(this).children().parent().removeClass("bg-gray-100").addClass("bg-white");
    $(".tab").addClass("cursor-pointer");
    $(this).removeClass("cursor-pointer");
    $(".dl-content").addClass("hidden");
    $(`#${id}-content`).removeClass("hidden");
    if (id == "docker") {
      $("#step2").hide();
      $("#step3").hide();
    } else {
      $("#step2").show();
      $("#step3").show();
    }
  });

  async function copyToClipboard(obj) {
    try {
      const cmd = $(obj).prev().children().eq(1).text();
      await window.navigator.clipboard.writeText(cmd);
      const popup = $(obj).next().show();
      setTimeout(() => $(obj).next().hide(), 1000);
    } catch (error) {
      console.log("copyToClipboard error!", error);
    }
  }

  const dropdowns = ["macos", "windows", "linux", "other"];

  $(document).on("click", function (event) {
    dropdowns.forEach((dropdown) => {
      if (!$(event.target).closest(`#${dropdown}-dropdown-picker`).length) {
        $(`#${dropdown}-dropdown`).hide();
      }
    });
  });

  function showDropdown(id) {
    $(`#${id}`).toggle();
  }
</script>



    
    
