<script type="text/javascript">
  $(function () {
    var userPlatform = navigator.platform.toUpperCase();
    if (userPlatform.indexOf("WIN") !== -1) {
      $("#windows").click();
    } else if (userPlatform.indexOf("LINUX") !== -1) {
      if (userPlatform.indexOf("386") !== -1) {
        $("#linux-selected").text("386");
        $("#linux-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-linux-386.tgz`);
      } else if (userPlatform.indexOf("ARM") !== -1) {
        $("#linux-selected").text("ARM");
        $("#linux-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-linux-arm.tgz`);
      }
      $("#linux").click();
    } else {
      $("#macos").click();
    }
  });
  $("#darwin_arm64").on("click", function () {
    $(this).parent().toggle();
    $("#macos-selected").text("Apple Silicon (ARM64)");
    $("#macos-extract").text("sudo unzip ~/Downloads/ngrok-stable-darwin-arm64.zip -d /usr/local/bin")
    $("#macos-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-darwin-arm64.zip`);
  });
  $("#darwin_amd64").on("click", function () {
    $(this).parent().toggle();
    $("#macos-selected").text("Intel (AMD64)");
    $("#macos-extract").text("sudo unzip ~/Downloads/ngrok-stable-darwin-amd64.zip -d /usr/local/bin")
    $("#macos-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-darwin-amd64.zip`);
  });
  $("#windows_amd64").on("click", function () {
    $(this).parent().toggle();
    $("#windows-selected").text("Windows (64-bit)");
    $("#windows-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-windows-amd64.zip`);
  });
  $("#windows_386").on("click", function () {
    $(this).parent().toggle();
    $("#windows-selected").text("Windows (32-bit)");
    $("#windows-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-windows-386.zip`);
  });
  $("#linux_amd64").on("click", function () {
    $(this).parent().toggle();
    $("#linux-selected").text("x86-64");
    $("#linux-extract").text("sudo tar xvzf ~/Downloads/ngrok-stable-linux-amd64.tgz -C /usr/local/bin")
    $("#linux-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-linux-amd64.tgz`);
  });
  $("#linux_386").on("click", function () {
    $(this).parent().toggle();
    $("#linux-selected").text("386");
    $("#linux-extract").text("sudo tar xvzf ~/Downloads/ngrok-stable-linux-386.tgz -C /usr/local/bin")
    $("#linux-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-linux-386.tgz`);
  });
  $("#linux_arm64").on("click", function () {
    $(this).parent().toggle();
    $("#linux-selected").text("ARM64");
    $("#linux-extract").text("sudo tar xvzf ~/Downloads/ngrok-stable-linux-arm64.tgz -C /usr/local/bin")
    $("#linux-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-linux-arm64.tgz`);
  });
  $("#linux_arm").on("click", function () {
    $(this).parent().toggle();
    $("#linux-selected").text("ARM");
    $("#linux-extract").text("sudo tar xvzf ~/Downloads/ngrok-stable-linux-arm.tgz -C /usr/local/bin")
    $("#linux-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-linux-arm.tgz`);
  });
  $("#linux_mips").on("click", function () {
    $(this).parent().toggle();
    $("#linux-selected").text("mips");
    $("#linux-extract").text("sudo tar xvzf ~/Downloads/ngrok-stable-linux-mips.tgz -C /usr/local/bin")
    $("#linux-dl-link").attr("href", `https://bin.equinox.io/c/4VmDzA7iaHb/ngrok-stable-linux-mips.tgz`);
